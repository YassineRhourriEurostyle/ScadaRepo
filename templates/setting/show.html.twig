{% block body %}
<h1>Setting</h1>
<div class="col">
    <a href="{{ path('setting_index') }}">Back to list</a> - 
    <a href="{{ path('setting_edit', {'id': setting.id}) }}">Edit</a>
</div>
<table class="no-filtering col-md-12 table table-sm table-bordered">
    <tbody>
                    <tr>
                <th>Id</th>
                <td>{{ setting.id }}</td>
            </tr>
                    <tr>
                <th>Iteration</th>
                <td>{{ setting.Iteration }}</td>
            </tr>
                    <tr>
                <th>Value</th>
                <td>{{ setting.Value }}</td>
            </tr>
            </tbody>
</table>

{{ include('setting/_delete_form.html.twig') }}
{% endblock %}
